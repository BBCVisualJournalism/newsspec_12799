            <h2 class="map1__title">{{ vocab.map1_title }}</h2>
            <div class="graveyard-map__subtitle graveyard-map__subtitle--one">
                <div class="graveyard-map__stats graveyard-map__stats--one">
                    <p class="graveyard-map__stats__number graveyard-map__stats__number--one">{{ vocab.map1_incidents_stat }}</p>
                    <p>{{ vocab.map_key_incidents }}</p>
                </div>
                <div class="graveyard-map__stats graveyard-map__stats--two">
