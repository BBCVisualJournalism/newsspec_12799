<section class="section section--two" id="section--two">

    <div style="display: none">

        <span id="drone_poster--english">{{ projectAssets }}/img/drone_video_poster_1040.jpg?v={{ vocab.version }}</span>
        <span id="drone_poster--other">{{ projectAssets }}/img/kato_tritos_poster_image_1040_no-text.jpg?v={{ vocab.version }}</span>
        <span id="lesbos_poster">{{ projectAssets }}/img/arrival_image_1040.jpg?v={{ vocab.version }}</span>
        <span id="voices_poster--english">{{ projectAssets }}/img/voices_of_lesbos_cover_1040_3.jpg?v={{ vocab.version }}</span>
        <span id="voices_poster--other">{{ projectAssets }}/img/voices_of_lesbos_cover_1040.jpg?v={{ vocab.version }}</span>
        <span id="fb_share_img">{{ projectAssets }}/img/italy_graves_intro_1400.jpg?v={{ vocab.version }}</span>

    </div>

    <div class="bbc-news-vj-video-wrapper">
        <div class="bbc-news-vj-video bbc-news-vj-video--drone" id="bbc-news-vj-video--drone">
            <div class="bbc-news-vj-video__player"></div>
        </div>
    </div>

    <div class="bbc-news-vj-grid-wrapper">
        <div class="bbc-news-vj-grid-wrapper__fluid-inner">
            <h2>{{{ vocab.section2_title }}}</h2>

            <p>{{{ vocab.section2_para_1 }}}</p>
            <p>{{{ vocab.section2_para_2 }}}</p>
            <p>{{{ vocab.section2_para_3 }}}</p>

            <img src="{{ projectAssets }}/img/firuz_640.jpg" width="100%" />
            <figcaption class="media-with-caption__caption">
                {{{ vocab.section2_img_1_caption }}}
            </figcaption>

            <p>{{{ vocab.section2_para_4 }}}</p>
            <p>{{{ vocab.section2_para_5 }}}</p>
            <p>{{{ vocab.section2_para_6 }}}</p>
            <p>{{{ vocab.section2_para_7 }}}</p>
            <p>{{{ vocab.section2_para_8 }}}</p>

            <h2>{{{ vocab.section2_title_2 }}}</h2>

        </div>
    </div>

    <div class="bbc-news-vj-grid-wrapper">
        <picture>
            <source media="(min-width: 1040px)" srcset="{{ projectAssets }}/img/maps/{{ language }}/lesbos_crossing_1040.jpg?v={{ vocab.version }}">
            <source media="(min-width: 810px)" srcset="{{ projectAssets }}/img/maps/{{ language }}/lesbos_crossing_810.jpg?v={{ vocab.version }}">
            <source media="(min-width: 624px)" srcset="{{ projectAssets }}/img/maps/{{ language }}/lesbos_crossing_624.jpg?v={{ vocab.version }}">

            <img class="lesbos-map__image__fallback" src="{{ projectAssets }}/img/maps/{{ language }}/lesbos_crossing_624.jpg?v={{ vocab.version }}"
                srcset="{{ projectAssets }}/img/maps/{{ language }}/lesbos_crossing_624.jpg?v={{ vocab.version }}"
                alt="{{ vocab.section_2_map_alt_text }}">
        </picture>
        <p class="lesbos-map__image__source-text">{{ vocab.responsive_image_source }}</p>

        <div class="bbc-news-vj-grid-wrapper__fluid-inner">
            <p>{{{ vocab.section2_para_9 }}}</p>
            <p>{{{ vocab.section2_para_10 }}}</p>
            <p>{{{ vocab.section2_para_11 }}}</p>
        </div>

        <img src="{{ projectAssets }}/img/firuz_family_photos_1040.jpg" />
